<resources>
    <string id="torqViewInterval">Report Frequency</string>
    <string id="torqView1s">1s</string>
    <string id="torqView3s">3s</string>
    <string id="torqView10s">10s</string>

    <properties>
        <property id="torqViewInterval" type="number">1</property>
    </properties>
    <settings>
        <setting propertyKey="@Properties.torqViewInterval" title="@Strings.torqViewInterval">
            <settingConfig type="list">
                <listEntry value="1">@Strings.torqView1s</listEntry>
                <listEntry value="3">@Strings.torqView3s</listEntry>
                <listEntry value="10">@Strings.torqView10s</listEntry>
            </settingConfig>
        </setting>
    </settings>
</resources>
